class Drug:
    
    def __init__(self, name, aisle, side, column, row):
        
        self.name = name
        self.aisle = aisle
        self.side = side
        self.column = column
        self.row = row
        
    def get_name(self):
        return self.name
    
    def get_aisle(self):
        return self.aisle
    
    def get_side(self):
        return self.side
    
    def get_column(self):
        return self.column
    
    def get_row(self):
        return self.row 

       
