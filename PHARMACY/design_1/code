class A(Frame):

    def createWidgets(self):
        
        Label(self, text="Choose a Drug", fg="red", font=("Ariel", 30, "bold")).grid(columnspan=4, sticky=W)
        Label(self, text="", fg="red", font=("Ariel", 30, "bold")).grid(columnspan=4, sticky=W)
                 
        self.btn1 = Button(self)    
        self.btn1["text"] = "Abilify" 
        self.btn1["font"]   = "Ariel", 20, "bold" 
        self.btn1["command"] = self.go_Abilify
        self.btn1.grid(row = 2, column = 0)

        self.btn2 = Button(self)    
        self.btn2["text"] = "Advair Diskus" 
        self.btn2["font"]   = "Ariel", 20, "bold" 
        self.btn2["command"] =  self.go_Advair_Diskus
        self.btn2.grid(row = 2, column = 1) 
        
        self.btn3 = Button(self)    
        self.btn3["text"] = "Alimta" 
        self.btn3["font"]   = "Ariel", 20, "bold"
        self.btn3["command"] = self.go_Alimta
        self.btn3.grid(row = 2, column = 2)
        
        self.btn4 = Button(self)    
        self.btn4["text"] = "Atripla" 
        self.btn4["font"]   = "Ariel", 20, "bold" 
        self.btn4["command"] = self.go_Atripla
        self.btn4.grid(row = 3, column = 0)
        
        self.btn5 = Button(self)    
        self.btn5["text"] = "Avastin" 
        self.btn5["font"]   = "Ariel", 20, "bold"
        self.btn5["command"] = self.go_Avastin
        self.btn5.grid(row = 3, column = 1)
        
        self.btn6 = Button(self)    
        self.btn6["text"] = "Avonex" 
        self.btn6["font"]   = "Ariel", 20, "bold"
        self.btn6["command"] = self.go_Avonex
        self.btn6.grid(row = 3, column = 2)
        
        self.btn7 = Button(self)    
        self.btn7["text"] = "Avonex Pen" 
        self.btn7["font"]   = "Ariel", 20, "bold"
        self.btn7["command"] = self.go_Avonex_Pen
        self.btn7.grid(row = 4, column = 1)
        
        self.back = Button(self)    
        self.back["text"] = "Back"  
        self.back["fg"]   = "red"    
        self.back["font"] = "Ariel", 20, "bold"  
        self.back["command"] =  self.go_back    
        self.back.grid(row = 5, column = 1)
        
    def go_Abilify(self):
        
        root2 = Toplevel(self.master)
        project = Abilify_description(root2)
        
    def go_Advair_Diskus(self):
        
        root2 = Toplevel(self.master)
        project = Advair_Diskus_description(root2)
        
    def go_Alimta(self):
        
        root2 = Toplevel(self.master)
        project = Alimta_description(root2)
        
    def go_Atripla(self):
        
        root2 = Toplevel(self.master)
        project = Atripla_description(root2)
        
    def go_Avastin(self):
        
        root2 = Toplevel(self.master)
        project = Avastin_description(root2)
        
    def go_Avonex(self):
        
        root2 = Toplevel(self.master)
        project = Avonex_description(root2)
        
    def go_Avonex_Pen(self):
        
        root2 = Toplevel(self.master)
        project = Avonex_Pen_description(root2)
              
    def go_back(self):
        
        self.master.destroy()
        
    def __init__(self, master = None):
        
        Frame.__init__(self, master)
        self.pack()
        self.createWidgets()
        
        
