        Label(self, text = ("Side:", Tecfidera.get_side()), fg="blue", font=("Ariel", 30, "bold")).grid(columnspan=4, sticky=W)
        Label(self, text = ("Column:", Tecfidera.get_column()), fg="blue", font=("Ariel", 30, "bold")).grid(columnspan=4, sticky=W)
        Label(self, text = ("Row:", Tecfidera.get_row()), fg="blue", font=("Ariel", 30, "bold")).grid(columnspan=4, sticky=W)   
        
        self.back = Button(self)    
        self.back["text"] = "Back"  
        self.back["fg"]   = "red"    
        self.back["font"] = "Ariel", 20, "bold"  
        self.back["command"] =  self.go_back    
        self.back.grid(row = 5, column = 0) 
        
    def go_back(self):
        
        self.master.destroy() 
    
    def __init__(self, master = None):
        
        Frame.__init__(self, master)
        self.pack()
        self.createWidgets()
        
